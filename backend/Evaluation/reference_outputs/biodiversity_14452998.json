  "FsF_F2_01M": {
    "test_results": {
      "FsF_F2_01M-1-2-3": {
        "creator": "Rehbein, Malte; Escobari Vargas, Andrea Belen; Fischer, Sarah; G\u0000fcntsch, Anton; Haas, Bettina; Matheisen, Giada; Perschl, Tobias; Wieshuber, Alois; Engel, Thore",
        "title": "Historical Animal Observation Records by Bavarian Forestry Offices (1845)",
        "publisher": "Zenodo",
        "publication_date": "2024-12-13",
        "summary": "In 1845, under Andreas Wagner's scientific leadership, the Bavarian government surveyed 44 selected vertebrate species across the country by sending questionnaires to all 119 forestry offices. For the first time, foresters' responses were systematically recorded and analyzed. The resulting dataset includes 5,467 geo-coded animal observations and stems from an interdisciplinary collaboration involving the University of Passau's Chair of Computational Humanities, the Bavarian State Archives, iDiv Halle-Jena-Leipzig, the Botanical Garden Berlin, and the NFDI4Biodiversity consortium.",
        "keywords": "Biodiversity, Historical Ecology, Computational Humanities, Bavaria, 19th Century, Digital humanities, Biodiversity conservation, Zoology"
      }
    }
  },
